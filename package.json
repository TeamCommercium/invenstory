    "cheerio": "^0.20.0",
    "cookie-parser": "^1.4.1",
    "d3": "^3.5.16",
    "dateformat": "^1.0.12",
    "enzyme": "^2.2.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "fs": "0.0.2",
    "history": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^0.4.3",
    "jsonwebtoken": "^5.7.0",
    "knex": "^0.10.0",
    "mws-sdk-promises": "^0.9.16",
    "noconsole": "^1.0.2",
    "nodemailer": "^2.4.1",
    "passport": "^0.3.2",
    "passport-amazon": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^4.5.5",
    "rd3": "^0.6.1",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-google-charts": "^0.2.1",
