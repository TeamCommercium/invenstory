{
  "name": "invenstory",
  "version": "0.0.1",
  "description": "Invenstory provides Amazon sellers with a useful information dashboard.",
  "main": "server.js",
  "scripts": {
    "lint": "node ./node_modules/eslint/bin/eslint ./client --ignore ./client/test",
    "postinstall": "mkdir -p dist && cp favicon.ico dist/; cp -R assets dist/",
    "pretest": "./node_modules/.bin/knex seed:run",
    "test": "./node_modules/.bin/mocha --compilers .:client/test/compiler.js,.:babel-core/register server/test client/test/ --recursive",
    "coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- ./server/test --recursive",
    "hot": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --progress --colors",
    "front": "./node_modules/webpack/bin/webpack.js --watch --progress --colors",
    "back": "./node_modules/babel-watch/babel-watch.js ./server/server.js",
    "start": "./node_modules/webpack/bin/webpack.js && ./node_modules/.bin/babel-node ./server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeamCommercium/invenstory.git"
  },
  "author": "Team Commercium",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TeamCommercium/invenstory/issues"
  },
  "homepage": "https://github.com/TeamCommercium/invenstory#readme",
